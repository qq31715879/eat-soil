<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hug.dao.mapper.UserMapper">

	<select id="selectById" resultType="com.hug.dao.bean.User">
		select * from user where id = #{id}
	</select>
	
	<select id="selectMap" resultType="com.hug.dao.bean.User">
		select id, name, nick_name from user u
	</select>

</mapper>  